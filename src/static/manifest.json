{
  "manifest_version": 3,
  "name": "Stock Price Targets",
  "description": "Qucik view of analysts predictions",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Stock Price Targets",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus", "search", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": [
        "https://stockcharts.com/*",
        "https://testext.netlify.app/api/getSite"
      ],
      "js": ["contentScript.js"]
    }
  ]
}
